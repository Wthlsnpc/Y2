@model IEnumerable<news2.Models.News>
@{
                /**/

                /**/

                ViewBag.Title = "News";
}

<div class="jumbotron">
    <h4>All news on this page</h4>
</div>
@foreach (var item in Model)
{
        <div style="border: 3px solid gray; padding: 5px; border-radius: 5px" >
            <p><strong>@Html.DisplayFor(modelItem => item.Head)</strong></p>
            <p>@Html.DisplayFor(modelItem => item.Content)</p>
            <p><em>@Html.DisplayFor(modelItem => item.Tags)</em></p>
        </div>
        <p></p>
}
